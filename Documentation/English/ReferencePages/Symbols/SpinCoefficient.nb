 CellLabel->"In[1]:=",ExpressionUUID->"e8b89dbc-72c9-472f-bd73-8cdd26024d3c"],

Cell[BoxData[
 RowBox[{
  RowBox[{"-", 
   FractionBox[
    RowBox[{"Cot", "[", "\[Theta]", "]"}], 
    RowBox[{
     SqrtBox["2"], " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"2", " ", "r"}], "-", 
       RowBox[{"2", " ", "\[ImaginaryI]", " ", "a", " ", 
        RowBox[{"Cos", "[", "\[Theta]", "]"}]}]}], ")"}]}]]}], "+", 
  FractionBox[
   RowBox[{"\[ImaginaryI]", " ", "a", " ", 
    RowBox[{"Sin", "[", "\[Theta]", "]"}]}], 
   RowBox[{
    SqrtBox["2"], " ", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"r", "-", 
       RowBox[{"\[ImaginaryI]", " ", "a", " ", 
        RowBox[{"Cos", "[", "\[Theta]", "]"}]}]}], ")"}], "2"]}]]}]], "Output",
 CellLabel->"Out[1]:= ",ExpressionUUID->"5c6d39f4-31f0-4088-a803-a3ff127b6275"]
}, Open  ]],


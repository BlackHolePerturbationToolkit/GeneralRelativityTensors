Cell[CellGroupData[{

Cell["\<\
A Curve is just a rank-1 contravariant Tensor with the flags \
\[OpenCurlyDoubleQuote]IsCurve\[CloseCurlyDoubleQuote] \[Rule] True, and \
\[OpenCurlyDoubleQuote]Curve\[CloseCurlyDoubleQuote]\[Rule] \
\[OpenCurlyDoubleQuote]Self\[CloseCurlyDoubleQuote]\
\>", "Item",ExpressionUUID->"92880539-606c-4506-8eca-7c13291f5252"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"List", "@@", "xK"}]], "Input",
 CellLabel->"In[5]:=",ExpressionUUID->"a3555757-bbc2-4d87-83fc-bc310d0629d4"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"\<\"AbstractQ\"\>", "\[Rule]", "False"}], ",", 
   RowBox[{"\<\"Coordinates\"\>", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"t", ",", "r", ",", "\[Theta]", ",", "\[Phi]"}], "}"}]}], ",", 
   RowBox[{"\<\"Curve\"\>", "\[Rule]", "\<\"Self\"\>"}], ",", 
   RowBox[{"\<\"CurveParameter\"\>", "\[Rule]", "\[Chi]"}], ",", 
   RowBox[{"\<\"CurveQ\"\>", "\[Rule]", "True"}], ",", 
   RowBox[{"\<\"Dimensions\"\>", "\[Rule]", "4"}], ",", 
   RowBox[{"\<\"DisplayName\"\>", "\[Rule]", "\<\"x\"\>"}], ",", 
   RowBox[{"\<\"Indices\"\>", "\[Rule]", 
    RowBox[{"{", "\[Alpha]", "}"}]}], ",", 
   RowBox[{"\<\"Metric\"\>", "\[Rule]", 
    TagBox[
     SubsuperscriptBox["g", "\[Alpha]\[Beta]", ""],
     DisplayForm]}], ",", 
   RowBox[{"\<\"MetricQ\"\>", "\[Rule]", "False"}], ",", 
   RowBox[{"\<\"Name\"\>", "\[Rule]", "\<\"EquatorialEccentricKerr\"\>"}], 
   ",", 
   RowBox[{"\<\"PossibleIndices\"\>", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{
     "\[Alpha]", ",", "\[Beta]", ",", "\[Gamma]", ",", "\[Delta]", ",", 
      "\[CurlyEpsilon]", ",", "\[Zeta]", ",", "\[Eta]", ",", "\[Iota]", ",", 
      "\[Kappa]", ",", "\[Lambda]", ",", "\[Mu]", ",", "\[Nu]", ",", "\[Xi]", 
      ",", "\[Omicron]", ",", "\[Rho]", ",", "\[Sigma]", ",", "\[FinalSigma]",
       ",", "\[Upsilon]", ",", "\[CurlyPhi]", ",", "\[Psi]", ",", 
      "\[Omega]"}], "}"}]}], ",", 
   RowBox[{"\<\"Tetrad\"\>", "\[Rule]", "Undefined"}], ",", 
   RowBox[{"\<\"Values\"\>", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"t", "[", "\[Chi]", "]"}], ",", 
      FractionBox[
       RowBox[{"M", " ", "p"}], 
       RowBox[{"1", "+", 
        RowBox[{"e", " ", 
         RowBox[{"Cos", "[", "\[Chi]", "]"}]}]}]], ",", 
      FractionBox["\[Pi]", "2"], ",", 
      RowBox[{"\[Phi]", "[", "\[Chi]", "]"}]}], "}"}]}]}], "}"}]], "Output",
 CellLabel->"Out[5]=",ExpressionUUID->"d669fc04-ae11-4099-ab64-8707294b5765"]
}, Open  ]]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["Putting other Tensors on Curves", \
"Section",ExpressionUUID->"1f79373a-77e3-48eb-9308-260a3608d2b6"],

Cell["\<\
We will often want a Tensor field (originally a function of all coordinates \
of the manifold) evaluated along a curve.\
\>", "Item",ExpressionUUID->"9d625671-5c85-4a9b-9fed-394caf259ffa"],

Cell[CellGroupData[{

Cell["\<\
As an example, consider the Tensor computed by ChristoffelSymbol.\
\>", "Item",ExpressionUUID->"1bffd861-7c2e-444c-923e-b0d17ad512a4"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"chrK", "=", 
  RowBox[{"ChristoffelSymbol", "[", 
   RowBox[{"gK", ",", 
    RowBox[{"ActWith", "\[Rule]", "Simplify"}]}], "]"}]}]], "Input",
 CellLabel->"In[6]:=",ExpressionUUID->"5aa4ceef-4239-4959-8f80-60fca9a375ae"],

Cell[BoxData[
 TagBox[
  SubsuperscriptBox["\[CapitalGamma]", 
   RowBox[{"  ", "\[Beta]\[Gamma]"}], 
   RowBox[{"\[Alpha]", "    "}]],
  DisplayForm]], "Output",
 CellLabel->"Out[6]=",ExpressionUUID->"c7d50145-8c73-47c4-93eb-329bcae1b20d"]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["\<\
This is a function of the four spacetime variables. For example, one \
component is:\
\>", "Item",ExpressionUUID->"7a64af5c-e647-4566-90e0-003acd47a004"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"chrK", "[", 
  RowBox[{"t", ",", 
   RowBox[{"-", "t"}], ",", 

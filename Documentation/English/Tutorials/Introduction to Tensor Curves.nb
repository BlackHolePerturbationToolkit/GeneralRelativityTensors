     RowBox[{
      StyleBox["c",
       FontFamily->"Times",
       FontWeight->"Regular",
       FontSlant->"Italic"], ".", " ", "Internally"}], " ", 
     StyleBox["t",
      FontFamily->"Times",
      FontWeight->"Regular",
      FontSlant->"Italic"], " ", "is", " ", "still", " ", "treated", " ", 
     "as", " ", "a", " ", "function", " ", "of", " ", "the", " ", 
     RowBox[{"manifold", "'"}], "s", " ", 

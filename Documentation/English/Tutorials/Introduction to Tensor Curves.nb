Cell[BoxData[
 RowBox[{"\<\"ChristoffelSymbol[m] returns the Christoffel symbol computed \
from the metric Tensor m.\"\>", "\[NonBreakingSpace]", 
  ButtonBox[
   StyleBox["\[RightSkeleton]", "SR"],
   Active->True,
   BaseStyle->"Link",
   ButtonData->
    "paclet:Tensors/ref/ChristoffelSymbol"]}]], "Print", "PrintUsage",
 CellTags->
  "Info63740435938-6633590",ExpressionUUID->"533ec05c-8de7-4be3-8161-\
439d72fcaad5"]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"chrK", "=", 
  RowBox[{"ChristoffelSymbol", "[", 
   RowBox[{"gK", ",", 
    RowBox[{"ActWith", "\[Rule]", "Simplify"}]}], "]"}]}]], "Input",
 CellLabel->"In[7]:=",ExpressionUUID->"90144554-5d47-48d1-8aab-a3c31ba1a0a1"],

Cell[BoxData[
 TagBox[
  SubsuperscriptBox["\[CapitalGamma]", 
   RowBox[{"  ", "\[Beta]\[Gamma]"}], 
   RowBox[{"\[Alpha]", "    "}]],
  DisplayForm]], "Output",
 CellLabel->"Out[7]=",ExpressionUUID->"e14edc31-88af-4ffb-955a-598fcc7770d2"]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["\<\
This is a function of the four spacetime variables. For example, one \
component is:\
\>", "Item",ExpressionUUID->"1913f954-8ccd-4190-bb77-41ae249f81d1"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"chrK", "[", 
  RowBox[{"t", ",", 
   RowBox[{"-", "t"}], ",", 
   RowBox[{"-", "r"}]}], "]"}]], "Input",
 CellLabel->"In[8]:=",ExpressionUUID->"5354ed25-0e14-45e5-ac84-1d863c1797b3"],

Cell[BoxData[
 RowBox[{"-", 
  FractionBox[
   RowBox[{"2", " ", "M", " ", 
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["a", "2"], "+", 
      SuperscriptBox["r", "2"]}], ")"}], " ", 
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["a", "2"], "-", 
      RowBox[{"2", " ", 
       SuperscriptBox["r", "2"]}], "+", 
      RowBox[{
       SuperscriptBox["a", "2"], " ", 
       RowBox[{"Cos", "[", 
        RowBox[{"2", " ", "\[Theta]"}], "]"}]}]}], ")"}]}], 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["a", "2"], "+", 
      RowBox[{"r", " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"-", "2"}], " ", "M"}], "+", "r"}], ")"}]}]}], ")"}], " ", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["a", "2"], "+", 
       RowBox[{"2", " ", 
        SuperscriptBox["r", "2"]}], "+", 
       RowBox[{
        SuperscriptBox["a", "2"], " ", 
        RowBox[{"Cos", "[", 
         RowBox[{"2", " ", "\[Theta]"}], "]"}]}]}], ")"}], 
     "2"]}]]}]], "Output",
 CellLabel->"Out[8]=",ExpressionUUID->"6f4eea5d-2f88-4f2c-96ae-5c97b7487768"]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["\<\
We can associate chrK with the Curve xK. This is done with \
ToTensorFieldOnCurve.\
\>", "Item",ExpressionUUID->"eaea5ecc-fa1c-4a2d-9bac-654ff7159e6d"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"?", "ToTensorFieldOnCurve"}]], "Input",
 CellLabel->"In[9]:=",ExpressionUUID->"0d897bec-ad8c-415f-87b7-efea57f3e3f1"],

Cell[BoxData[
 RowBox[{"\<\"ToTensorFieldOnCurve[t,c] returns the Tensor t with associated \
Curve c. Internally t is still treated as a function of the manifold's \
coordinates.\"\>", "\[NonBreakingSpace]", 
  ButtonBox[
   StyleBox["\[RightSkeleton]", "SR"],
   Active->True,
   BaseStyle->"Link",
   ButtonData->
    "paclet:Tensors/ref/ToTensorFieldOnCurve"]}]], "Print", "PrintUsage",
 CellTags->
  "Info93740435940-6633590",ExpressionUUID->"3c7b02b4-f3a1-4fc1-a4d1-\
58b5c7a2cc09"]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"chrKF", "=", 
  RowBox[{"ToTensorFieldOnCurve", "[", 
   RowBox[{"chrK", ",", "xK"}], "]"}]}]], "Input",

Cell[CellGroupData[{

Cell["\<\
A Curve is just a rank-1 contravariant Tensor with the flags \
\[OpenCurlyDoubleQuote]IsCurve\[CloseCurlyDoubleQuote] \[Rule] True, and \
\[OpenCurlyDoubleQuote]Curve\[CloseCurlyDoubleQuote]\[Rule] \
\[OpenCurlyDoubleQuote]Self\[CloseCurlyDoubleQuote]\
\>", "Item",ExpressionUUID->"505f83cf-baee-4d20-9a8b-a499d69278b6"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"List", "@@", "xK"}]], "Input",
 CellLabel->"In[5]:=",ExpressionUUID->"fdb64307-3ddb-4928-a96a-5d6caff121e3"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"\<\"AbstractQ\"\>", "\[Rule]", "False"}], ",", 
   RowBox[{"\<\"Coordinates\"\>", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"t", ",", "r", ",", "\[Theta]", ",", "\[Phi]"}], "}"}]}], ",", 
   RowBox[{"\<\"Curve\"\>", "\[Rule]", "\<\"Self\"\>"}], ",", 
   RowBox[{"\<\"CurveParameter\"\>", "\[Rule]", "\[Chi]"}], ",", 
   RowBox[{"\<\"CurveQ\"\>", "\[Rule]", "True"}], ",", 
   RowBox[{"\<\"Dimensions\"\>", "\[Rule]", "4"}], ",", 
   RowBox[{"\<\"DisplayName\"\>", "\[Rule]", "\<\"x\"\>"}], ",", 
   RowBox[{"\<\"Indices\"\>", "\[Rule]", 
    RowBox[{"{", "\[Alpha]", "}"}]}], ",", 
   RowBox[{"\<\"Metric\"\>", "\[Rule]", 
    TagBox[
     SubsuperscriptBox["g", "\[Alpha]\[Beta]", ""],
     DisplayForm]}], ",", 
   RowBox[{"\<\"MetricQ\"\>", "\[Rule]", "False"}], ",", 
   RowBox[{"\<\"Name\"\>", "\[Rule]", "\<\"EquatorialEccentricKerr\"\>"}], 
   ",", 
   RowBox[{"\<\"PossibleIndices\"\>", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{
     "\[Alpha]", ",", "\[Beta]", ",", "\[Gamma]", ",", "\[Delta]", ",", 
      "\[CurlyEpsilon]", ",", "\[Zeta]", ",", "\[Eta]", ",", "\[Iota]", ",", 
      "\[Kappa]", ",", "\[Lambda]", ",", "\[Mu]", ",", "\[Nu]", ",", "\[Xi]", 
      ",", "\[Omicron]", ",", "\[Rho]", ",", "\[Sigma]", ",", "\[FinalSigma]",
       ",", "\[Upsilon]", ",", "\[CurlyPhi]", ",", "\[Psi]", ",", 
      "\[Omega]"}], "}"}]}], ",", 
   RowBox[{"\<\"Values\"\>", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"t", "[", "\[Chi]", "]"}], ",", 
      FractionBox[
       RowBox[{"M", " ", "p"}], 
       RowBox[{"1", "+", 
        RowBox[{"e", " ", 
         RowBox[{"Cos", "[", "\[Chi]", "]"}]}]}]], ",", 
      FractionBox["\[Pi]", "2"], ",", 
      RowBox[{"\[Phi]", "[", "\[Chi]", "]"}]}], "}"}]}]}], "}"}]], "Output",
 CellLabel->"Out[5]=",ExpressionUUID->"a1023a75-4116-4dad-ac7a-440ea21ac41f"]
}, Open  ]]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["Putting other Tensors on Curves", \
"Section",ExpressionUUID->"c0dd0128-33b6-45c6-89de-467d7ad315d6"],

Cell["\<\
We will often want a Tensor field (originally a function of all coordinates \
of the manifold) evaluated along a curve.\
\>", "Item",ExpressionUUID->"7f385388-0ab1-4582-ad83-3172238a289f"],

Cell[CellGroupData[{

Cell["\<\
As an example, consider the Tensor computed by ChristoffelSymbol.\
\>", "Item",ExpressionUUID->"586ffcd4-ef0d-4385-a68e-9bed6a9ba070"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"chrK", "=", 
  RowBox[{"ChristoffelSymbol", "[", 
   RowBox[{"gK", ",", 
    RowBox[{"ActWith", "\[Rule]", "Simplify"}]}], "]"}]}]], "Input",
 CellLabel->"In[6]:=",ExpressionUUID->"2f3fa7fc-4e82-41f5-a18b-2bb07e6c8da8"],

Cell[BoxData[
 TagBox[
  SubsuperscriptBox["\[CapitalGamma]", 
   RowBox[{"  ", "\[Beta]\[Gamma]"}], 
   RowBox[{"\[Alpha]", "    "}]],
  DisplayForm]], "Output",
 CellLabel->"Out[6]=",ExpressionUUID->"bb00c8c3-1640-4e1b-9694-84e550fbe766"]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["\<\
This is a function of the four spacetime variables. For example, one \
component is:\
\>", "Item",ExpressionUUID->"edec640f-8098-4566-ac50-a732f1169311"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"chrK", "[", 
  RowBox[{"t", ",", 
   RowBox[{"-", "t"}], ",", 
   RowBox[{"-", "r"}]}], "]"}]], "Input",
 CellLabel->"In[7]:=",ExpressionUUID->"c8ed2311-cb8d-437f-a7fb-6a61ccc380fb"],

Cell[BoxData[
 RowBox[{"-", 
  FractionBox[
   RowBox[{"2", " ", "M", " ", 
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["a", "2"], "+", 
      SuperscriptBox["r", "2"]}], ")"}], " ", 
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["a", "2"], "-", 
      RowBox[{"2", " ", 

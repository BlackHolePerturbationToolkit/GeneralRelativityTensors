Cell[CellGroupData[{

Cell["Tensors evaluated explicitly on Curves", \
"Section",ExpressionUUID->"4480c7df-4b69-40c1-ad75-8a4fbd851c93"],

Cell["\<\
Sometimes we will want to evaluate the values of a Tensor explicitly along a \
Curve.\
\>", "Item",ExpressionUUID->"05ecb59e-2ca1-4a94-84a7-713f86089bd5"],

Cell["For instance, the four-velocity is defined only on a Curve.", \
"Item",ExpressionUUID->"1b12443c-c04e-4a9f-9130-2cca4a4b4a4c"],

Cell[TextData[{
 "Consider a generic ",
 StyleBox["trajectory",
  IgnoreSpellCheck->True],
 " in Schwarzschild, parametrized by proper time \[Tau]."
}], "Item",ExpressionUUID->"c7541b1d-66e6-4fa3-bcc4-d037f2a88a86"],

Cell[CellGroupData[{

Cell["Without loss of generality, \[Theta]=\[Pi]/2.", \
"Item",ExpressionUUID->"791b84e4-7503-40db-82f9-2c6a021abc50"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"xS", "=", 
  RowBox[{"ToCurve", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"\"\<GenericSchwarzschildPosition\>\"", ",", "\"\<x\>\""}], 
     "}"}], ",", 
    RowBox[{"ToMetric", "[", "\"\<Schw\>\"", "]"}], ",", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"t", "[", "\[Tau]", "]"}], ",", 
      RowBox[{"r", "[", "\[Tau]", "]"}], ",", 
      RowBox[{"\[Pi]", "/", "2"}], ",", 
      RowBox[{"\[Phi]", "[", "\[Tau]", "]"}]}], "}"}], ",", "\[Tau]"}], 
   "]"}]}]], "Input",
 CellLabel->"In[14]:=",ExpressionUUID->"3088dd01-56b5-473c-89b1-3c92656e4a84"],

Cell[BoxData[
 TagBox[
  RowBox[{
   SubsuperscriptBox["x", "", "\[Alpha]"], "[", "\<\"\[Tau]\"\>", "]"}],
  DisplayForm]], "Output",
 CellLabel->
  "Out[14]=",ExpressionUUID->"8f878526-777d-4c68-8bc2-28bc008a377e"]
}, Open  ]]
}, Open  ]],


Cell[CellGroupData[{

Cell["Tensors evaluated explicitly on Curves", \
"Section",ExpressionUUID->"77c90b55-01d2-43ea-859a-e22fbe9c54c8"],

Cell["\<\
Sometimes we will want to evaluate the values of a Tensor explicitly along a \
Curve.\
\>", "Item",ExpressionUUID->"59458329-2681-42e8-9b12-415db28e2fba"],

Cell["For instance, the four-velocity is defined only on a Curve.", \
"Item",ExpressionUUID->"9b2d5bf2-42f4-436d-b716-00de77d5fe0f"],

Cell[TextData[{
 "Consider a generic ",
 StyleBox["trajectory",
  IgnoreSpellCheck->True],
 " in Schwarzschild, parametrized by proper time \[Tau]."
}], "Item",ExpressionUUID->"3b1e1b04-cfb3-4dde-bdfc-336ebea84c03"],

Cell[CellGroupData[{

Cell["Without loss of generality, \[Theta]=\[Pi]/2.", \
"Item",ExpressionUUID->"fa543283-b175-42f5-a338-697751cde96c"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"xS", "=", 
  RowBox[{"ToCurve", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"\"\<GenericSchwarzschildPosition\>\"", ",", "\"\<x\>\""}], 
     "}"}], ",", 
    RowBox[{"ToMetric", "[", "\"\<Schw\>\"", "]"}], ",", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"t", "[", "\[Tau]", "]"}], ",", 
      RowBox[{"r", "[", "\[Tau]", "]"}], ",", 
      RowBox[{"\[Pi]", "/", "2"}], ",", 
      RowBox[{"\[Phi]", "[", "\[Tau]", "]"}]}], "}"}], ",", "\[Tau]"}], 
   "]"}]}]], "Input",
 CellLabel->"In[14]:=",ExpressionUUID->"3032eea4-01a7-4431-89b7-0b4eec34061d"],

Cell[BoxData[
 TagBox[
  RowBox[{
   SubsuperscriptBox["x", "", "\[Alpha]"], "[", "\<\"\[Tau]\"\>", "]"}],
  DisplayForm]], "Output",
 CellLabel->
  "Out[14]=",ExpressionUUID->"19fcba47-a495-43fd-91c6-3dc13cf3aa3c"]
}, Open  ]]
}, Open  ]],


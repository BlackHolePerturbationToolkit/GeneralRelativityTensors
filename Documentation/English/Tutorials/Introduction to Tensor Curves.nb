Cell[CellGroupData[{

Cell["\<\
Here, we find the geodesic equations for Schwarzschild by computing the \
covariant derivative of the 4-velocity along itself.\
\>", "Item",ExpressionUUID->"37c75e41-f374-4543-a0db-ec22522547f3"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"uGradU", "=", 
  RowBox[{"CovariantD", "[", 
   RowBox[{"uS", ",", "uS"}], "]"}]}]], "Input",
 CellLabel->"In[25]:=",ExpressionUUID->"2ff21727-0c2d-4c9a-99d4-011d1c98278f"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   TagBox[
    RowBox[{
     SubsuperscriptBox["\[CapitalGamma]", 
      RowBox[{"  ", "\[Beta]\[Gamma]"}], 
      RowBox[{"\[Alpha]", "    "}]], "[", "\<\"\[Tau]\"\>", "]"}],
    DisplayForm], " ", 
   TagBox[
    RowBox[{
     SubsuperscriptBox["u", "", "\[Beta]"], "[", "\<\"\[Tau]\"\>", "]"}],
    DisplayForm], " ", 
   TagBox[
    RowBox[{
     SubsuperscriptBox["u", "", "\[Gamma]"], "[", "\<\"\[Tau]\"\>", "]"}],
    DisplayForm]}], "+", 
  TagBox[
   RowBox[{
    SubsuperscriptBox[
     RowBox[{"(", 
      RowBox[{"du", "/", "d\[Tau]"}], ")"}], "", "\[Alpha]"], 
    "[", "\<\"\[Tau]\"\>", "]"}],
   DisplayForm]}]], "Output",
 CellLabel->
  "Out[25]=",ExpressionUUID->"a3b7aff8-9496-47d2-9840-81ad9ce2c1ef"]
}, Open  ]]
}, Open  ]],


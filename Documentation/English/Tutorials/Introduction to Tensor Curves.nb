Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"chrK", "=", 
  RowBox[{"ChristoffelSymbol", "[", 
   RowBox[{"gK", ",", 
    RowBox[{"ActWith", "\[Rule]", "Simplify"}]}], "]"}]}]], "Input",
 CellLabel->"In[7]:=",ExpressionUUID->"4a2f239e-e757-472d-bba9-4d27dda42211"],

Cell[BoxData[

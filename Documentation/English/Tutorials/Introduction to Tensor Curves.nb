        RowBox[{"1", "+", 
         RowBox[{"e", " ", 
          RowBox[{"Cos", "[", "\[Chi]", "]"}]}]}], ")"}], "2"]]}], ")"}]}], 
  RowBox[{"M", " ", 
   SuperscriptBox["p", "2"], " ", 
   RowBox[{"(", 
    RowBox[{
     SuperscriptBox["a", "2"], "+", 
     FractionBox[
      RowBox[{"M", " ", "p", " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"-", "2"}], " ", "M"}], "+", 
         FractionBox[
          RowBox[{"M", " ", "p"}], 
          RowBox[{"1", "+", 
           RowBox[{"e", " ", 
            RowBox[{"Cos", "[", "\[Chi]", "]"}]}]}]]}], ")"}]}], 
      RowBox[{"1", "+", 
       RowBox[{"e", " ", 
        RowBox[{"Cos", "[", "\[Chi]", "]"}]}]}]]}], ")"}]}]]], "Output",
 CellLabel->
  "Out[11]=",ExpressionUUID->"3c83a696-6be6-46b7-a7ae-44030f7d8663"]
}, Open  ]]
}, Open  ]],


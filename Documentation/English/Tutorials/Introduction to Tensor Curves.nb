Cell[CellGroupData[{

Cell["\<\
A Curve is just a rank-1 contravariant Tensor with the flags \
\[OpenCurlyDoubleQuote]IsCurve\[CloseCurlyDoubleQuote] \[Rule] True, and \
\[OpenCurlyDoubleQuote]Curve\[CloseCurlyDoubleQuote]\[Rule] \
\[OpenCurlyDoubleQuote]Self\[CloseCurlyDoubleQuote]\
\>", "Item",ExpressionUUID->"4168c4d8-81dd-4916-b438-9e4dd904445f"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"List", "@@", "xK"}]], "Input",
 CellLabel->"In[5]:=",ExpressionUUID->"2ebedb58-fb7f-4bbd-9ca9-ebf71d010f7d"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"\<\"Abstract\"\>", "\[Rule]", "False"}], ",", 
   RowBox[{"\<\"Coordinates\"\>", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"t", ",", "r", ",", "\[Theta]", ",", "\[Phi]"}], "}"}]}], ",", 
   RowBox[{"\<\"Curve\"\>", "\[Rule]", "\<\"Self\"\>"}], ",", 
   RowBox[{"\<\"CurveParameter\"\>", "\[Rule]", "\[Chi]"}], ",", 
   RowBox[{"\<\"Dimensions\"\>", "\[Rule]", "4"}], ",", 
   RowBox[{"\<\"DisplayName\"\>", "\[Rule]", "\<\"x\"\>"}], ",", 
   RowBox[{"\<\"Indices\"\>", "\[Rule]", 
    RowBox[{"{", "\[Alpha]", "}"}]}], ",", 
   RowBox[{"\<\"IsCurve\"\>", "\[Rule]", "True"}], ",", 
   RowBox[{"\<\"IsMetric\"\>", "\[Rule]", "False"}], ",", 
   RowBox[{"\<\"Metric\"\>", "\[Rule]", 
    TagBox[
     SubsuperscriptBox["g", "\[Alpha]\[Beta]", ""],
     DisplayForm]}], ",", 
   RowBox[{"\<\"Name\"\>", "\[Rule]", "\<\"EquatorialEccentricKerr\"\>"}], 
   ",", 
   RowBox[{"\<\"PossibleIndices\"\>", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{
     "\[Alpha]", ",", "\[Beta]", ",", "\[Gamma]", ",", "\[Delta]", ",", 
      "\[CurlyEpsilon]", ",", "\[Zeta]", ",", "\[Eta]", ",", "\[Iota]", ",", 
      "\[Kappa]", ",", "\[Lambda]", ",", "\[Mu]", ",", "\[Nu]", ",", "\[Xi]", 
      ",", "\[Omicron]", ",", "\[Rho]", ",", "\[Sigma]", ",", "\[FinalSigma]",
       ",", "\[Upsilon]", ",", "\[CurlyPhi]", ",", "\[Psi]", ",", 
      "\[Omega]"}], "}"}]}], ",", 
   RowBox[{"\<\"Values\"\>", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"t", "[", "\[Chi]", "]"}], ",", 
      FractionBox[
       RowBox[{"M", " ", "p"}], 
       RowBox[{"1", "+", 
        RowBox[{"e", " ", 
         RowBox[{"Cos", "[", "\[Chi]", "]"}]}]}]], ",", 
      FractionBox["\[Pi]", "2"], ",", 
      RowBox[{"\[Phi]", "[", "\[Chi]", "]"}]}], "}"}]}]}], "}"}]], "Output",
 CellLabel->"Out[5]=",ExpressionUUID->"e22560fc-a71c-4d9f-b979-f7889b197ddc"]
}, Open  ]]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["Putting other Tensors on Curves", \
"Section",ExpressionUUID->"6b233026-b268-4cc3-a353-5b6c5b7de940"],

Cell["\<\
We will often want a Tensor field (originally a function of all coordinates \
of the manifold) evaluated along a curve.\
\>", "Item",ExpressionUUID->"6bd2762e-8bc5-4b5a-ad37-c3531fa9331e"],

Cell[CellGroupData[{

Cell["\<\
As an example, consider the Tensor computed by ChristoffelSymbol.\
\>", "Item",ExpressionUUID->"09aa6354-8312-4919-b60d-93ab058dae17"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"chrK", "=", 
  RowBox[{"ChristoffelSymbol", "[", 
   RowBox[{"gK", ",", 
    RowBox[{"ActWith", "\[Rule]", "Simplify"}]}], "]"}]}]], "Input",
 CellLabel->"In[6]:=",ExpressionUUID->"292af83c-51e1-47a1-a3f8-9a4c29d5f0f2"],

Cell[BoxData[
 TagBox[
  SubsuperscriptBox["\[CapitalGamma]", 
   RowBox[{"  ", "\[Beta]\[Gamma]"}], 
   RowBox[{"\[Alpha]", "    "}]],
  DisplayForm]], "Output",
 CellLabel->"Out[6]=",ExpressionUUID->"02df2e67-1cb9-476c-9025-2ad75799a29d"]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["\<\
This is a function of the four spacetime variables. For example, one \
component is:\
\>", "Item",ExpressionUUID->"eb8e5933-cdab-4099-949e-f2ab5562bab4"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"chrK", "[", 
  RowBox[{"t", ",", 
   RowBox[{"-", "t"}], ",", 
   RowBox[{"-", "r"}]}], "]"}]], "Input",
 CellLabel->"In[7]:=",ExpressionUUID->"1ffee0df-5118-4628-8111-30e6358e5e10"],

Cell[BoxData[
 RowBox[{"-", 
  FractionBox[
   RowBox[{"2", " ", "M", " ", 
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["a", "2"], "+", 
      SuperscriptBox["r", "2"]}], ")"}], " ", 
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["a", "2"], "-", 
      RowBox[{"2", " ", 
       SuperscriptBox["r", "2"]}], "+", 
      RowBox[{
       SuperscriptBox["a", "2"], " ", 
       RowBox[{"Cos", "[", 
        RowBox[{"2", " ", "\[Theta]"}], "]"}]}]}], ")"}]}], 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["a", "2"], "+", 
      RowBox[{"r", " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"-", "2"}], " ", "M"}], "+", "r"}], ")"}]}]}], ")"}], " ", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["a", "2"], "+", 
       RowBox[{"2", " ", 
        SuperscriptBox["r", "2"]}], "+", 
       RowBox[{
        SuperscriptBox["a", "2"], " ", 
        RowBox[{"Cos", "[", 
         RowBox[{"2", " ", "\[Theta]"}], "]"}]}]}], ")"}], 
     "2"]}]]}]], "Output",
 CellLabel->"Out[7]=",ExpressionUUID->"ce47c1f8-825a-475e-9483-e385d44343c4"]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["\<\
We can associate this field with our Curve xK. This is done with \
ToTensorFieldOnCurve.\

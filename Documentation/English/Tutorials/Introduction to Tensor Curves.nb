Cell[CellGroupData[{

Cell["Tensors evaluated explicitly on Curves", \
"Section",ExpressionUUID->"74a56306-2243-4f98-85ae-417de3701cc3"],

Cell["\<\
Sometimes we will want to evaluate the values of a Tensor explicitly along a \
Curve.\
\>", "Item",ExpressionUUID->"41065771-9437-44ed-845c-2e8766e10ce5"],

Cell["For instance, the four-velocity is defined only on a Curve.", \
"Item",ExpressionUUID->"da7e7877-a94d-48ae-a69d-776faf502599"],

Cell[TextData[{
 "Consider a generic ",
 StyleBox["trajectory",
  IgnoreSpellCheck->True],
 " in Schwarzschild, parametrized by proper time \[Tau]."
}], "Item",ExpressionUUID->"d1ddf497-d4c7-4d4f-82bc-f09942147cc8"],

Cell[CellGroupData[{

Cell["Without loss of generality, \[Theta]=\[Pi]/2.", \
"Item",ExpressionUUID->"8218177f-3d83-4519-a8dd-018ef836d3a7"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"xS", "=", 
  RowBox[{"ToCurve", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"\"\<GenericSchwarzschildPosition\>\"", ",", "\"\<x\>\""}], 
     "}"}], ",", 
    RowBox[{"ToMetric", "[", "\"\<Schw\>\"", "]"}], ",", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"t", "[", "\[Tau]", "]"}], ",", 
      RowBox[{"r", "[", "\[Tau]", "]"}], ",", 
      RowBox[{"\[Pi]", "/", "2"}], ",", 
      RowBox[{"\[Phi]", "[", "\[Tau]", "]"}]}], "}"}], ",", "\[Tau]"}], 
   "]"}]}]], "Input",
 CellLabel->"In[14]:=",ExpressionUUID->"bce4f24f-d93d-4f28-a759-abb6aa050f7f"],

Cell[BoxData[
 TagBox[
  RowBox[{
   SubsuperscriptBox["x", "", "\[Alpha]"], "[", "\<\"\[Tau]\"\>", "]"}],
  DisplayForm]], "Output",
 CellLabel->
  "Out[14]=",ExpressionUUID->"0581f46b-b886-4598-94f0-c4d9d09424a8"]
}, Open  ]]
}, Open  ]],


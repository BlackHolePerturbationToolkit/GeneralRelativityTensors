Cell[CellGroupData[{

Cell["\<\
Here, we find the geodesic equations for Schwarzschild by computing the \
covariant derivative of the 4-velocity along itself.\
\>", "Item",ExpressionUUID->"3bdd0bcf-988d-4511-8f62-d471a60b1241"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"uGradU", "=", 
  RowBox[{"CovariantD", "[", 
   RowBox[{"uS", ",", "uS"}], "]"}]}]], "Input",
 CellLabel->"In[25]:=",ExpressionUUID->"8bfb713f-e0d0-4670-8966-3e4d7faddf81"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   TagBox[
    RowBox[{
     SubsuperscriptBox["\[CapitalGamma]", 
      RowBox[{"  ", "\[Beta]\[Gamma]"}], 
      RowBox[{"\[Alpha]", "    "}]], "[", "\<\"\[Tau]\"\>", "]"}],
    DisplayForm], " ", 
   TagBox[
    RowBox[{
     SubsuperscriptBox["u", "", "\[Beta]"], "[", "\<\"\[Tau]\"\>", "]"}],
    DisplayForm], " ", 
   TagBox[
    RowBox[{
     SubsuperscriptBox["u", "", "\[Gamma]"], "[", "\<\"\[Tau]\"\>", "]"}],
    DisplayForm]}], "+", 
  TagBox[
   RowBox[{
    SubsuperscriptBox[
     RowBox[{"(", 
      RowBox[{"du", "/", "d\[Tau]"}], ")"}], "", "\[Alpha]"], 
    "[", "\<\"\[Tau]\"\>", "]"}],
   DisplayForm]}]], "Output",
 CellLabel->
  "Out[25]=",ExpressionUUID->"77f941ab-91a9-4879-91f9-4f68ec18c163"]
}, Open  ]]
}, Open  ]],


Cell[CellGroupData[{

Cell["Tensors evaluated explicitly on Curves", \
"Section",ExpressionUUID->"a3c358ef-a3a0-4fb5-91a5-282ba8a6b47d"],

Cell["\<\
Sometimes we will want to evaluate the values of a Tensor explicitly along a \
Curve.\
\>", "Item",ExpressionUUID->"ac492c21-e515-45dc-b749-b64cf6c58a60"],

Cell["For instance, the four-velocity is defined only on a Curve.", \
"Item",ExpressionUUID->"706130f0-a9e0-49b6-b8ee-327cd9af7802"],

Cell[TextData[{
 "Consider a generic ",
 StyleBox["trajectory",
  IgnoreSpellCheck->True],
 " in Schwarzschild, parametrized by proper time \[Tau]."
}], "Item",ExpressionUUID->"ea6e16cc-82a3-4672-9505-f61f375278ae"],

Cell[CellGroupData[{

Cell["Without loss of generality, \[Theta]=\[Pi]/2.", \
"Item",ExpressionUUID->"d4f40dbc-8937-4583-a8ed-7e0e43fe7ad7"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"xS", "=", 
  RowBox[{"ToCurve", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"\"\<GenericSchwarzschildPosition\>\"", ",", "\"\<x\>\""}], 
     "}"}], ",", 
    RowBox[{"ToMetric", "[", "\"\<Schw\>\"", "]"}], ",", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"t", "[", "\[Tau]", "]"}], ",", 
      RowBox[{"r", "[", "\[Tau]", "]"}], ",", 
      RowBox[{"\[Pi]", "/", "2"}], ",", 
      RowBox[{"\[Phi]", "[", "\[Tau]", "]"}]}], "}"}], ",", "\[Tau]"}], 
   "]"}]}]], "Input",
 CellLabel->"In[14]:=",ExpressionUUID->"7240f0b9-068c-48ba-be02-bc68f9561908"],

Cell[BoxData[
 TagBox[
  RowBox[{
   SubsuperscriptBox["x", "", "\[Alpha]"], "[", "\<\"\[Tau]\"\>", "]"}],
  DisplayForm]], "Output",
 CellLabel->
  "Out[14]=",ExpressionUUID->"dd8b4d94-373e-4787-9713-1d1cfd1268e2"]
}, Open  ]]
}, Open  ]],


 CellLabel->"In[4]:=",ExpressionUUID->"f8404447-03bd-48fd-bb57-92fe35d1d1c2"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"0.023223`", ",", 
   RowBox[{
    TagBox[
     SubsuperscriptBox["g", "", "\[Alpha]\[Beta]"],
     DisplayForm], " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{"-", 

Cell[CellGroupData[{

Cell["\<\
In order to simplify this further, we need to make use of the eigenvalue \
equations for Ylm. This allows us to remove derivatives.\
\>", "Item",ExpressionUUID->"c73ded9f-9264-4a60-a4a9-209d16be0ea7"],

Cell[BoxData[{
 RowBox[{"Clear", "[", 
  RowBox[{"d2thYRule", ",", "dphYRule"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"d2thYRule", "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"Derivative", "[", 
       RowBox[{
        RowBox[{"a_", "/;", 
         RowBox[{"a", "\[GreaterEqual]", "2"}]}], ",", "b_"}], "]"}], "[", 
      "Ylm", "]"}], "[", 
     RowBox[{"\[Theta]", ",", "\[Phi]"}], "]"}], "\[RuleDelayed]", 
    RowBox[{"D", "[", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          FractionBox[
           SuperscriptBox["m", "2"], 
           SuperscriptBox[
            RowBox[{"Sin", "[", "\[Theta]", "]"}], "2"]], "-", 
          RowBox[{"l", " ", 
           RowBox[{"(", 
            RowBox[{"1", "+", "l"}], ")"}]}]}], ")"}], 
        RowBox[{"Ylm", "[", 
         RowBox[{"\[Theta]", ",", "\[Phi]"}], "]"}]}], "-", 
       RowBox[{
        FractionBox[
         RowBox[{"Cos", "[", "\[Theta]", "]"}], 
         RowBox[{"Sin", "[", "\[Theta]", "]"}]], 
        RowBox[{"D", "[", 
         RowBox[{
          RowBox[{"Ylm", "[", 
           RowBox[{"\[Theta]", ",", "\[Phi]"}], "]"}], ",", "\[Theta]"}], 
         "]"}]}]}], ",", 
      RowBox[{"{", 
       RowBox[{"\[Theta]", ",", 
        RowBox[{"a", "-", "2"}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"\[Phi]", ",", "b"}], "}"}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"dphYRule", "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"Derivative", "[", 
       RowBox[{"a_", ",", "b_"}], "]"}], "[", "Ylm", "]"}], "[", 
     RowBox[{"\[Theta]", ",", "\[Phi]"}], "]"}], "\[RuleDelayed]", 
    RowBox[{"D", "[", 
     RowBox[{
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"I", " ", "m"}], ")"}], "b"], 
       RowBox[{"Ylm", "[", 
        RowBox[{"\[Theta]", ",", "\[Phi]"}], "]"}]}], ",", 
      RowBox[{"{", 
       RowBox[{"\[Theta]", ",", "a"}], "}"}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"removeYlmDerivs", "[", "expr_", "]"}], ":=", 
   RowBox[{"expr", "//.", 
    RowBox[{"{", 
     RowBox[{"d2thYRule", ",", "dphYRule"}], "}"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"collectAndSimp", "[", "expr_", "]"}], ":=", 
  RowBox[{"Collect", "[", 
   RowBox[{
    RowBox[{"removeYlmDerivs", "@", "expr"}], ",", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"_", "[", 
       RowBox[{"\[Theta]", ",", "\[Phi]"}], "]"}], ",", 
      RowBox[{"_", "[", 
       RowBox[{"t", ",", "r"}], "]"}]}], "}"}], ",", "Simplify"}], 
   "]"}]}]}], "Input",
 CellLabel->"In[7]:=",ExpressionUUID->"634d6eab-4ab5-4f66-b2c2-e3e0bfab79db"]
}, Open  ]],


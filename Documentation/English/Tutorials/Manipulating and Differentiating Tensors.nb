Cell[BoxData[
 InterpretationBox[
  RowBox[{"\<\"Cannot add Tensors with different indices, \"\>", 
   "\[InvisibleSpace]", 
   RowBox[{"{", 
    RowBox[{"\[Alpha]", ",", "\[Beta]"}], "}"}], 

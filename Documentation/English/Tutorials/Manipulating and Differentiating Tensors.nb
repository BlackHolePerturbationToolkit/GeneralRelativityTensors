 CellLabel->"In[14]:=",ExpressionUUID->"869e3a18-9e8b-414a-9825-bf04488b0c7e"],

Cell[BoxData[
 InterpretationBox[
  RowBox[{"\<\"Cannot add Tensors with different indices, \"\>", 
   "\[InvisibleSpace]", 
   RowBox[{"{", 
    RowBox[{"\[Alpha]", ",", "\[Beta]"}], "}"}], 

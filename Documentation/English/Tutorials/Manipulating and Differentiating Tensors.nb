Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"gS", "[", 
    RowBox[{"\[Alpha]", ",", 
     RowBox[{"-", "\[Beta]"}]}], "]"}], "//", "TensorValues"}], "//", 
  "MatrixForm"}]], "Input",
 CellLabel->"In[5]:=",ExpressionUUID->"a523b679-8e54-4ace-b603-a9dc27489abd"],

Cell[BoxData[
 TagBox[
  RowBox[{"(", "\[NoBreak]", GridBox[{
     {
      FractionBox[
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", "1"}], "+", 

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"gS", "[", 
  RowBox[{"\[Alpha]", ",", "\[Beta]"}], "]"}]], "Input",
 CellLabel->"In[5]:=",ExpressionUUID->"52ec04b1-dac1-4e1a-aab5-98933f3ce27e"],

Cell[BoxData[
 TagBox[
  SubsuperscriptBox["g", "", "\[Alpha]\[Beta]"],

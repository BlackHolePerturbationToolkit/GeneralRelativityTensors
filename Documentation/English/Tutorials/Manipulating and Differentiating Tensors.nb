Cell["\<\
Those specific indices are arbitrary until they are traced over or used in an \
operation with another Tensor\
\>", "Item",ExpressionUUID->"b1129410-2ed8-4381-b084-67df63540547"],

Cell[CellGroupData[{

Cell["\<\
That is, simply writing the following will not change the Tensor\
\[CloseCurlyQuote]s values internally, even though the order of the two \
indices has changed\
\>", "Item",ExpressionUUID->"c398e8d6-096e-449d-a0b9-5bb80e1c6a81"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"fsRN", "[", 
  RowBox[{
   RowBox[{"-", "\[Beta]"}], ",", 
   RowBox[{"-", "\[Alpha]"}]}], "]"}]], "Input",
 CellLabel->"In[37]:=",ExpressionUUID->"71fb9457-0cc2-4830-bf8a-5e708527ffdf"],

Cell[BoxData[
 TagBox[
  SubsuperscriptBox["F", "\[Beta]\[Alpha]", ""],
  DisplayForm]], "Output",
 CellLabel->
  "Out[37]=",ExpressionUUID->"0e19c10a-ae40-4979-9a9e-99200a722270"]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{


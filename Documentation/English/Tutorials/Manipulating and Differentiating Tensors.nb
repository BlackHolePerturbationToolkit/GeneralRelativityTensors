Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"gS", "[", 
    RowBox[{"\[Alpha]", ",", 
     RowBox[{"-", "\[Beta]"}]}], "]"}], "//", "TensorValues"}], "//", 
  "MatrixForm"}]], "Input",
 CellLabel->"In[5]:=",ExpressionUUID->"111163bc-bcc8-486f-8a33-78317c50e428"],

Cell[BoxData[
 TagBox[
  RowBox[{"(", "\[NoBreak]", GridBox[{
     {
      FractionBox[
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", "1"}], "+", 

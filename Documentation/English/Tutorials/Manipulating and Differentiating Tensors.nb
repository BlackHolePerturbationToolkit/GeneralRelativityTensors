Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"gS", "[", 
    RowBox[{"\[Alpha]", ",", 
     RowBox[{"-", "\[Beta]"}]}], "]"}], "//", "TensorValues"}], "//", 
  "MatrixForm"}]], "Input",
 CellLabel->"In[5]:=",ExpressionUUID->"1b3b35e9-6f16-4b0a-bc6b-d8190c9e36ae"],

Cell[BoxData[
 TagBox[
  RowBox[{"(", "\[NoBreak]", GridBox[{
     {
      FractionBox[
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", "1"}], "+", 
          FractionBox[
           RowBox[{"2", " ", 
            TagBox[
             RowBox[{"Removed", "[", "\<\"M\"\>", "]"}],

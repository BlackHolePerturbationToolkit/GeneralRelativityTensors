 CellLabel->"In[14]:=",ExpressionUUID->"a778ffa6-2763-4f17-90f3-89e9f78e071f"],

Cell[BoxData[
 InterpretationBox[
  RowBox[{"\<\"Cannot add Tensors with different indices, \"\>", 
   "\[InvisibleSpace]", 
   RowBox[{"{", 
    RowBox[{"\[Alpha]", ",", "\[Beta]"}], "}"}], 
   "\[InvisibleSpace]", "\<\" and \"\>", "\[InvisibleSpace]", 
   RowBox[{"{", 
    RowBox[{"\[Beta]", ",", "\[Gamma]"}], "}"}]}],
  SequenceForm[
  "Cannot add Tensors with different indices, ", {
   Temp`\[Alpha], Temp`\[Beta]}, " and ", {Temp`\[Beta], Temp`\[Gamma]}],

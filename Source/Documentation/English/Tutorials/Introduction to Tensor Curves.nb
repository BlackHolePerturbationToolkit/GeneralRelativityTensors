 CellLabel->"In[30]:=",ExpressionUUID->"ba447ab1-f01b-421c-a44c-0059768d9b73"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   TagBox[
    RowBox[{
     SubsuperscriptBox["\[CapitalGamma]", 
      RowBox[{"  ", "\[Beta]\[Gamma]"}], 
      RowBox[{"\[Alpha]", "    "}]], "[", "\<\"\[Tau]\"\>", "]"}],
    DisplayForm], " ", 
   TagBox[
    RowBox[{
     SubsuperscriptBox["u", "", "\[Beta]"], "[", "\<\"\[Tau]\"\>", "]"}],
    DisplayForm], " ", 
   TagBox[
    RowBox[{
     SubsuperscriptBox["u", "", "\[Gamma]"], "[", "\<\"\[Tau]\"\>", "]"}],
    DisplayForm]}], "+", 
  TagBox[

Notebook[{

Cell[CellGroupData[{
Cell["GeneralRelativityTensors - Introduction to Tensor Curves", \
"Title",ExpressionUUID->"79330fa4-c775-4612-b1a1-bc24cfab5a9d"],

Cell["\<\
GeneralRelativityTensors provides support for Tensors defined on curves.\
\>", "Text",ExpressionUUID->"42f863cd-2dc8-4622-bc12-4ec323d50f54"],

Cell[BoxData[
 RowBox[{"<<", "GeneralRelativityTensors`"}]], "Input",
 CellLabel->"In[1]:=",ExpressionUUID->"d6aedce6-0558-4f94-847d-94a79ad24bfa"],

Cell[CellGroupData[{

Cell["Defining Curves", \
"Section",ExpressionUUID->"1621da8f-61f5-4887-bd6a-197f73a18166"],

Cell["\<\
A Curve must have an associated metric. As an example, here we use Kerr \
spacetime.\
\>", "Item",ExpressionUUID->"fea11879-97a5-4249-8ea4-97b5023d308d"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"gK", "=", 
  RowBox[{"ToMetric", "[", "\"\<Kerr\>\"", "]"}]}]], "Input",
 CellLabel->"In[2]:=",ExpressionUUID->"83730266-b95c-4f79-bb70-4e6596a060e6"],

Cell[BoxData[
 TagBox[
  SubsuperscriptBox["g", "\[Alpha]\[Beta]", ""],
  DisplayForm]], "Output",
 CellLabel->"Out[2]=",ExpressionUUID->"6ed9ae2e-b31e-4c1c-9ea6-55f63750832e"]
}, Open  ]],

Cell["\<\
Consider a parametrized equatorial, eccentric orbit on Kerr. The curve \
parameter is \[Chi].\
\>", "Item",ExpressionUUID->"594971f4-ad80-47f5-88ac-1e1d33bbe37a"],

Cell[BoxData[
 RowBox[{
  RowBox[{"eccKerrVals", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"t", "[", "\[Chi]", "]"}], ",", 
     FractionBox[
      RowBox[{"p", " ", "M"}], 
      RowBox[{"1", "+", 
       RowBox[{"e", " ", 
        RowBox[{"Cos", "[", "\[Chi]", "]"}]}]}]], ",", 
     RowBox[{"\[Pi]", "/", "2"}], ",", 
     RowBox[{"\[Phi]", "[", "\[Chi]", "]"}]}], "}"}]}], ";"}]], "Input",
 CellLabel->"In[3]:=",ExpressionUUID->"ccb99bea-5d6c-441c-85b4-8e4f69568130"],

Cell["\<\
The Curve is defined on our manifold with the function ToCurve.\
\>", "Item",ExpressionUUID->"e451876a-4ee1-4546-b144-1adc52becc24"],

Cell[BoxData[GridBox[{
   {
    RowBox[{
     ButtonBox["ToCurve",
      BaseStyle->"Link",
      ButtonData->"paclet:GeneralRelativityTensors/ref/ToCurve"], "[", 
     StyleBox[
      RowBox[{"n", ",", "m", ",", "vals", ",", "param"}], "TI"], "]"}], Cell[
    TextData[{
     "Returns Tensor Curve with name ",
     StyleBox["n",
      FontFamily->"Times",
      FontWeight->"Regular",
      FontSlant->"Italic"],
     " on the metric ",
     StyleBox["m",
      FontFamily->"Times",
      FontWeight->"Regular",
      FontSlant->"Italic"],
     StyleBox[".",
      FontFamily->"Times",
      FontWeight->"Regular"],
     " Its values are ",
     StyleBox["vals",
      FontFamily->"Times",
      FontWeight->"Regular",
      FontSlant->"Italic"],
     ", and pararmeter is ",
     StyleBox["param.",
      FontFamily->"Times",
      FontWeight->"Regular",
      FontSlant->"Italic"]
    }], "TableText",ExpressionUUID->"231243c4-9c68-45a3-843c-7a7c85165842"]}
  }]], "DefinitionBox",
 CellID->1334,ExpressionUUID->"542341ec-c7e8-4fcf-9fb0-975f14449016"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"xK", "=", 
  RowBox[{"ToCurve", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"\"\<EquatorialEccentricKerr\>\"", ",", "\"\<x\>\""}], "}"}], 
    ",", "gK", ",", "eccKerrVals", ",", "\[Chi]"}], "]"}]}]], "Input",
 CellLabel->"In[4]:=",ExpressionUUID->"0e31f0db-c0ae-4154-b943-6f24b345c6a6"],

Cell[BoxData[
 TagBox[
  RowBox[{
   SubsuperscriptBox["x", "", "\[Alpha]"], "[", "\<\"\[Chi]\"\>", "]"}],
  DisplayForm]], "Output",
 CellLabel->"Out[4]=",ExpressionUUID->"9220a415-8053-40df-b559-267411965f99"]
}, Open  ]],

Cell["\<\
A Curve is just a rank-1 contravariant Tensor with the flags \
\[OpenCurlyDoubleQuote]IsCurve\[CloseCurlyDoubleQuote] \[Rule] True, and \
\[OpenCurlyDoubleQuote]Curve\[CloseCurlyDoubleQuote]\[Rule] \
\[OpenCurlyDoubleQuote]Self\[CloseCurlyDoubleQuote]\
\>", "Item",ExpressionUUID->"e6677684-abc2-4b58-9def-963d324b727b"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"List", "@@", "xK"}]], "Input",
 CellLabel->"In[5]:=",ExpressionUUID->"02959f60-2714-4732-a029-e224d800701e"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"\<\"Coordinates\"\>", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"t", ",", "r", ",", "\[Theta]", ",", "\[Phi]"}], "}"}]}], ",", 
   RowBox[{"\<\"Metric\"\>", "\[Rule]", 
    TagBox[
     SubsuperscriptBox["g", "\[Alpha]\[Beta]", ""],
     DisplayForm]}], ",", 
   RowBox[{"\<\"IsMetric\"\>", "\[Rule]", "False"}], ",", 
   RowBox[{"\<\"Name\"\>", "\[Rule]", "\<\"EquatorialEccentricKerr\"\>"}], 
   ",", 
   RowBox[{"\<\"DisplayName\"\>", "\[Rule]", "\<\"x\"\>"}], ",", 
   RowBox[{"\<\"Indices\"\>", "\[Rule]", 
    RowBox[{"{", "\[Alpha]", "}"}]}], ",", 
   RowBox[{"\<\"PossibleIndices\"\>", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{
     "\[Alpha]", ",", "\[Beta]", ",", "\[Gamma]", ",", "\[Delta]", ",", 
      "\[CurlyEpsilon]", ",", "\[Zeta]", ",", "\[Eta]", ",", "\[Iota]", ",", 
      "\[Kappa]", ",", "\[Lambda]", ",", "\[Mu]", ",", "\[Nu]", ",", "\[Xi]", 
      ",", "\[Omicron]", ",", "\[Rho]", ",", "\[Sigma]", ",", "\[FinalSigma]",
       ",", "\[Upsilon]", ",", "\[CurlyPhi]", ",", "\[Psi]", ",", 
      "\[Omega]"}], "}"}]}], ",", 
   RowBox[{"\<\"Abstract\"\>", "\[Rule]", "False"}], ",", 
   RowBox[{"\<\"Values\"\>", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"t", "[", "\[Chi]", "]"}], ",", 
      FractionBox[
       RowBox[{"M", " ", "p"}], 
       RowBox[{"1", "+", 
        RowBox[{"e", " ", 
         RowBox[{"Cos", "[", "\[Chi]", "]"}]}]}]], ",", 
      FractionBox["\[Pi]", "2"], ",", 
      RowBox[{"\[Phi]", "[", "\[Chi]", "]"}]}], "}"}]}], ",", 
   RowBox[{"\<\"Dimensions\"\>", "\[Rule]", "4"}], ",", 
   RowBox[{"\<\"IsCurve\"\>", "\[Rule]", "True"}], ",", 
   RowBox[{"\<\"Curve\"\>", "\[Rule]", "\<\"Self\"\>"}], ",", 
   RowBox[{"\<\"CurveParameter\"\>", "\[Rule]", "\[Chi]"}]}], "}"}]], "Output",
 CellLabel->"Out[5]=",ExpressionUUID->"b2657c1b-2779-424a-aa4f-ed2e7c767d8b"]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["Putting other Tensors on Curves", \
"Section",ExpressionUUID->"a30ddf5e-7484-4e74-ab33-2f58ba238ffd"],

Cell[CellGroupData[{

Cell["\<\
We will often want a Tensor field (originally a function of all coordinates \
of the manifold) evaluated along a curve.\
\>", "Item",ExpressionUUID->"700b97c4-6b19-41d4-8019-a67bff42c890"],

Cell["\<\
As an example, consider the Tensor computed by ChristoffelSymbol.\
\>", "Item",ExpressionUUID->"4b2b8a5b-3507-4444-b788-a8d1134336a2"]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"chrK", "=", 
  RowBox[{"ChristoffelSymbol", "[", 
   RowBox[{"gK", ",", 
    RowBox[{"ActWith", "\[Rule]", "Simplify"}]}], "]"}]}]], "Input",
 CellLabel->"In[7]:=",ExpressionUUID->"dbcc272f-45d1-4023-b58c-0e055109a492"],

Cell[BoxData[
 TagBox[
  SubsuperscriptBox["\[CapitalGamma]", 
   RowBox[{"  ", "\[Beta]\[Gamma]"}], 
   RowBox[{"\[Alpha]", "    "}]],
  DisplayForm]], "Output",
 CellLabel->"Out[7]=",ExpressionUUID->"3499d83e-6442-46c4-b642-cce62bdc7f56"]
}, Open  ]],

Cell["\<\
This is a function of the four spacetime variables. For example, one \
component is:\
\>", "Item",ExpressionUUID->"264cb7ee-fc9f-4c9b-a30c-7eb84bdc48b8"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"chrK", "[", 
  RowBox[{"t", ",", 
   RowBox[{"-", "t"}], ",", 
   RowBox[{"-", "r"}]}], "]"}]], "Input",
 CellLabel->"In[8]:=",ExpressionUUID->"d59db9e3-df2f-45c0-aea3-320003e5d0d1"],

Cell[BoxData[
 RowBox[{"-", 
  FractionBox[
   RowBox[{"2", " ", "M", " ", 
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["a", "2"], "+", 
      SuperscriptBox["r", "2"]}], ")"}], " ", 
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["a", "2"], "-", 
      RowBox[{"2", " ", 
       SuperscriptBox["r", "2"]}], "+", 
      RowBox[{
       SuperscriptBox["a", "2"], " ", 
       RowBox[{"Cos", "[", 
        RowBox[{"2", " ", "\[Theta]"}], "]"}]}]}], ")"}]}], 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["a", "2"], "+", 
      RowBox[{"r", " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"-", "2"}], " ", "M"}], "+", "r"}], ")"}]}]}], ")"}], " ", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["a", "2"], "+", 
       RowBox[{"2", " ", 
        SuperscriptBox["r", "2"]}], "+", 
       RowBox[{
        SuperscriptBox["a", "2"], " ", 
        RowBox[{"Cos", "[", 
         RowBox[{"2", " ", "\[Theta]"}], "]"}]}]}], ")"}], 
     "2"]}]]}]], "Output",
 CellLabel->"Out[8]=",ExpressionUUID->"8540d1e6-3e5f-4034-a0e0-b7a0235a7e48"]
}, Open  ]],

Cell["\<\
We can associate this field with our Curve xK. This is done with \
ToTensorFieldOnCurve.\
\>", "Item",ExpressionUUID->"a97167a7-d053-43d0-8da5-cea87b690310"],

Cell[BoxData[GridBox[{
   {
    RowBox[{
     ButtonBox["ToTensorFieldOnCurve",
      BaseStyle->"Link",
      ButtonData->"paclet:GeneralRelativityTensors/ref/ToTensorFieldOnCurve"],
      "[", 
     StyleBox[
      RowBox[{"t", ",", "c"}], "TI"], "]"}], 
    RowBox[{"Returns", " ", "the", " ", "Tensor", " ", 
     StyleBox["t",
      FontFamily->"Times",
      FontWeight->"Regular",
      FontSlant->"Italic"], " ", "with", " ", "associated", " ", "Curve", " ", 
     RowBox[{
      StyleBox["c",
       FontFamily->"Times",
       FontWeight->"Regular",
       FontSlant->"Italic"], ".", " ", "Internally"}], " ", 
     StyleBox["t",
      FontFamily->"Times",
      FontWeight->"Regular",
      FontSlant->"Italic"], " ", "is", " ", "still", " ", "treated", " ", 
     "as", " ", "a", " ", "function", " ", "of", " ", "the", " ", 
     RowBox[{"manifold", "'"}], "s", " ", 
     RowBox[{"coordinates", "."}]}]}
  }]], "DefinitionBox",
 CellID->546406789,ExpressionUUID->"4b9ed791-3ef4-45a6-a9ae-dde704e640d9"],


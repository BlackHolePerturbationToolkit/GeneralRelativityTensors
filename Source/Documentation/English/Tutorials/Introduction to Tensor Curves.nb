 CellLabel->"In[30]:=",ExpressionUUID->"2331dcc7-9084-450f-a0b0-e76dfc298951"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   TagBox[
    RowBox[{
     SubsuperscriptBox["\[CapitalGamma]", 
      RowBox[{"  ", "\[Beta]\[Gamma]"}], 
      RowBox[{"\[Alpha]", "    "}]], "[", "\<\"\[Tau]\"\>", "]"}],
    DisplayForm], " ", 
   TagBox[
    RowBox[{
     SubsuperscriptBox["u", "", "\[Beta]"], "[", "\<\"\[Tau]\"\>", "]"}],

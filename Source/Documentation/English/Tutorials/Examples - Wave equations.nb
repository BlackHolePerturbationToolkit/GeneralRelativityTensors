Cell[BoxData[
 RowBox[{"MatrixForm", "[", 
  RowBox[{"AVecVals", "=", 
   RowBox[{"Join", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"At", "[", 
         RowBox[{"t", ",", "r"}], "]"}], ",", 
        RowBox[{"Ar", "[", 
         RowBox[{"t", ",", "r"}], "]"}]}], "}"}], 
      RowBox[{"Ylm", "[", 
       RowBox[{"\[Theta]", ",", "\[Phi]"}], "]"}]}], ",", 
     RowBox[{
      RowBox[{
       RowBox[{"AOdd", "[", 
        RowBox[{"t", ",", "r"}], "]"}], 
       RowBox[{"TensorValues", "[", 
        RowBox[{
         RowBox[{"TensorSphericalHarmonic", "[", "\"\<XA\>\"", "]"}], "[", 
         "A", "]"}], "]"}]}], "+", 
      RowBox[{
       RowBox[{"AEven", "[", 
        RowBox[{"t", ",", "r"}], "]"}], 
